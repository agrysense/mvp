var farmhouseManager = require("../entities/farmhouses/farmhouseManager");
var log = require("log"); log.setLevel("info");

var params =  request.parameters;
if (params.body) {
    params = JSON.parse(params.body);
}

try {
    var fhm1 = new farmhouseManager.FarmhouseManager();
    var farmhouse1 = fhm1.getFarmhouse(params);
    if (farmhouse1.key != null) {
        return fhm1.deleteFarmhouse(farmhouse1);
	}
}catch(exception) {
    return exception;
}