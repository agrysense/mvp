<schema>
	<aclGroups>
		<aclGroup name='user'>
			<read>user</read>
			<write>user</write>
			<fields>
				<field>type</field>
				<field>value</field>
				<field>time</field>
			</fields>
		</aclGroup>
		<schemaAcl>
			<read>nobody</read>
			<write>nobody</write>
			<delete>nobody</delete>
		</schemaAcl>
	</aclGroups>
	<fields>
		<field name='type' type='string'>
			<validation>
				<cardinality min='1' max='1'/>
			</validation>
		</field>
		<field name='value' type='numeric'>
			<validation>
				<cardinality min='1' max='1'/>
			</validation>
		</field>
		<field name='time' type='date'>
			<validation>
				<cardinality min='1' max='1'/>
			</validation>
		</field>
	</fields>
</schema>
