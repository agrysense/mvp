<schema>
	<aclGroups>
		<aclGroup name='user'>
			<read>user</read>
			<write>user</write>
			<fields>
			    <field>id</field>
				<field>controller</field>
				<field>embedded</field>
				<field>third_party</field>
				<field>vendor</field>
				<field>model</field>
			</fields>
		</aclGroup>
		<schemaAcl>
			<read>nobody</read>
			<write>nobody</write>
			<delete>nobody</delete>
		</schemaAcl>
	</aclGroups>
	<fields>
	    <field name='id' type='string'>
			<validation>
				<cardinality min='1' max='1'/>
			</validation>
		</field>
		<field name='controller' type='string'>
			<validation>
				<cardinality min='1' max='1'/>
			</validation>
		</field>
		<field name='embedded' type='string'>
			<validation>
				<cardinality min='1' max='1'/>
			</validation>
		</field>
		<field name='third_party' type='string'>
			<validation>
				<cardinality min='1' max='1'/>
			</validation>
		</field>
		<field name='vendor' type='string'>
			<validation>
				<cardinality min='1' max='1'/>
			</validation>
		</field>
		<field name='model' type='string'>
			<validation>
				<cardinality min='1' max='1'/>
			</validation>
		</field>
	</fields>
</schema>
