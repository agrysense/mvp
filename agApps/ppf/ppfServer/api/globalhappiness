var log = require("log"); log.setLevel("info");
var farmManagerModule = require("../entities/farms/farmManager");

try {
    
    var farmManager = new farmManagerModule.FarmManager();
    var happiness = farmManager.calculateGlobalHappiness();
    publish("responseChannel", {"id": "global_happyness", "result": happiness});
    return happiness;
}catch(exception){
    return exception;
}